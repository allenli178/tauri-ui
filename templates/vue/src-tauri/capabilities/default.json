{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "notification:allow-is-permission-granted",
    "notification:default",
    "os:allow-os-type",
    "store:default",
    "os:allow-version",
    "app:allow-name",
    "os:allow-arch"
  ]
}
