{
  "productName": "tauri-ui",
  "version": "0.0.0",
  "identifier": "com.yourname.dev",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": false,
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "title": "tauri-ui-demo",
        "center": true,
        "resizable": true,
        "decorations": false,
        "transparent": true,
        "skipTaskbar": false,
        "height": 1200,
        "width": 1000
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
